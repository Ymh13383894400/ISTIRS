<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.String?>
<?import views.MyToolTip?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="790.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ImageView fitHeight="40.0" pickOnBounds="true"
			preserveRatio="true">
			<HBox.margin>
				<Insets left="10.0" />
			</HBox.margin>
			<image>
				<Image url="/resources/renwu3.png" />
			</image>
		</ImageView>
		<VBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="150.0">
			<children>
				<Label fx:id="project_name" prefHeight="20.0"
					prefWidth="150.0" style="-fx-font-size:18;-fx-text-fill: #4f4f4f;">
				</Label>
				<Label fx:id="runningInfo" prefHeight="18.0" prefWidth="440.0"
					style="-fx-font-size:13;-fx-text-fill: #4f4f4f;">
				</Label>
			</children>
			<HBox.margin>
				<Insets left="10" />
			</HBox.margin>
		</VBox>
		<ProgressIndicator fx:id="run" prefHeight="16.0"
			prefWidth="20.0" visible="false">
			<HBox.margin>
				<Insets left="60.0" />
			</HBox.margin>
		</ProgressIndicator>
		<JFXButton fx:id="paramDetail" text="%ParamDetail">
			<HBox.margin>
				<Insets left="330.0" />
			</HBox.margin>
		</JFXButton>
		<JFXButton fx:id="close"
			style="-fx-padding: 4;-fx-pref-width:26 ;-fx-focus-color: transparent;">
			<tooltip>
				<MyToolTip text="%Shut"></MyToolTip>
			</tooltip>
			<graphic>
				<ImageView fx:id="img_hide" fitWidth="24.0"
					preserveRatio="true" smooth="true">
					<image>
						<Image url="/resources/cancel.png" />
					</image>
				</ImageView>
			</graphic>
			<HBox.margin>
				<Insets left="30.0" />
			</HBox.margin>
		</JFXButton>
	</children>
</HBox>
